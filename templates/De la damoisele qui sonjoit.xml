					<app>
						<lem wit="#Transcription">Ausi comme en songes estoit,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">En va celui en sa meson,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">Si c'onques ne li oï on :</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">Tant quist que li trova son lit.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">Gros avoit et quarré le cit</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">Et mout ert cointes, liez et baut.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
