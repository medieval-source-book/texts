	<text>
		<front>
			<head>
				<title>
					<app>
						<lem wit="#Transcription">Ein ander wis</lem>
						<rdg wit="#Translation"><!--TITLE'S TRANSLATION--></rdg>
					</app>
				</title>
			</head>
		</front>
		<body>
			<lg type="stanza" n="1">
				<l n="1">
					<app>
						<lem wit="#Transcription">Nu ist der laid winder hie,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="2">
					<app>
						<lem wit="#Transcription">des verdriuzt iunge zu den alten.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="3">
					<app>
						<lem wit="#Transcription">welch rat wirt der chlainen vogelin?</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="4">
					<app>
						<lem wit="#Transcription">man gesah mich stolczer nie.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="5">
					<app>
						<lem wit="#Transcription">hat diu heide rosen vur behalten,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="6">
					<app>
						<lem wit="#Transcription">do man in dem mayen siht erschinen,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="7">
					<app>
						<lem wit="#Transcription">den chinden sing ich niwen sanch.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="8">
					<app>
						<lem wit="#Transcription">daz wirt aber Weirat ein epheltranch</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="9">
					<app>
						<lem wit="#Transcription">e daz siz gelerne,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="10">
					<app>
						<lem wit="#Transcription">wan diu horet min geplæcze gerne.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="2">
				<l n="11">
					<app>
						<lem wit="#Transcription">Nu wol uf chinder, welt ir dar</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="12">
					<app>
						<lem wit="#Transcription">in den mayerhof zu Hadmuten,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="13">
					<app>
						<lem wit="#Transcription">da verwæn ich mich der massenu&esup;:</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="14">
					<app>
						<lem wit="#Transcription">Engelpreht und Adelwan,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="15">
					<app>
						<lem wit="#Transcription">Friderich in der gazzen, Tu&esup;ce,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="16">
					<app>
						<lem wit="#Transcription">Wentel und ir swester alle dri,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="17">
					<app>
						<lem wit="#Transcription">Hilpurch, ein vil schonez chint,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="18">
					<app>
						<lem wit="#Transcription">Jeutel und ir mumen tohte Ermelint.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="19">
					<app>
						<lem wit="#Transcription">Trouten swester Bryde</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="20">
					<app>
						<lem wit="#Transcription">split mit Eppen umb eim vingeride.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="3">
				<l n="21">
					<app>
						<lem wit="#Transcription">Ez ist noch niht vol ein iar,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="22">
					<app>
						<lem wit="#Transcription">daz si saz und vrowen genuege.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="23">
					<app>
						<lem wit="#Transcription">da begund si mich sain gruzzen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="24">
					<app>
						<lem wit="#Transcription">u&oring;f min triwe, daz ist war,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="25">
					<app>
						<lem wit="#Transcription">si gap mir ze choufen in dem chru&esup;ge,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="26">
					<app>
						<lem wit="#Transcription">da mit ich den minen schaden buzze.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="27">
					<app>
						<lem wit="#Transcription">idoch mus ich ze iungist sagen,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="28">
					<app>
						<lem wit="#Transcription">da wart wunder sleg uf min hant geslagen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="29">
					<app>
						<lem wit="#Transcription">so si sei z'uneren!</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="30">
					<app>
						<lem wit="#Transcription">daz was Reihczenfut mit Uchsengeren.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="4">
				<l n="31">
					<app>
						<lem wit="#Transcription">Waz ich durch den guten chneht</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="32">
					<app>
						<lem wit="#Transcription">niwer schuch durchel han gemachet</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="33">
					<app>
						<lem wit="#Transcription">und vil mangen liehten tach versoumet,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="34">
					<app>
						<lem wit="#Transcription">den si da heizzent Engelpreht.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="35">
					<app>
						<lem wit="#Transcription">der giht und er si mit mir verswachet.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="36">
					<app>
						<lem wit="#Transcription">doch hat si im sin strazze geroumet.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="37">
					<app>
						<lem wit="#Transcription">seht, des gie ir grouzziu not,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="38">
					<app>
						<lem wit="#Transcription">wand er chau si tægelich vur schonez brot.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="39">
					<app>
						<lem wit="#Transcription">we, wi ez mir erbarmet,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="40">
					<app>
						<lem wit="#Transcription">daz ir vuez bi vromdem viwer erwarmet!</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lglg type="stanza" n="5">
				<l n="41">
					<app>
						<lem wit="#Transcription">Ouf der linden leit mayl.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="42">
					<app>
						<lem wit="#Transcription">do von ist der walt des loubez ane</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="43">
					<app>
						<lem wit="#Transcription">und diu nahtegal ir hercz twinget.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="44">
					<app>
						<lem wit="#Transcription">wirt si mir, so han ich hail.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="45">
					<app>
						<lem wit="#Transcription">die ich da main, daz ist diu wolgetane,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="46">
					<app>
						<lem wit="#Transcription">diu mir min gemu&esup;t diche rignet.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="47">
					<app>
						<lem wit="#Transcription">wol ir, daz si sælich si!</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="48">
					<app>
						<lem wit="#Transcription">swer si minnet, der belibt sorgen vri.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="49">
					<app>
						<lem wit="#Transcription">si ist unwandelbære.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="50">
					<app>
						<lem wit="#Transcription">witen garten tu&esup;t si ru&esup;ben lære.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="6">
				<l n="51">
					<app>
						<lem wit="#Transcription">Stund iz noch an miner wal,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="52">
					<app>
						<lem wit="#Transcription">so næm ich die schonen z'einer vrowen,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="53">
					<app>
						<lem wit="#Transcription">der ich mich doch nimmer wil verzeihen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="54">
					<app>
						<lem wit="#Transcription">chumt si mir ze Riwental,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="55">
					<app>
						<lem wit="#Transcription">si mach grozzen mangel wol da schowen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="56">
					<app>
						<lem wit="#Transcription">von dem ebenhous unz an die richen</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="57">
					<app>
						<lem wit="#Transcription">da stet iz leider allez bloz.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="58">
					<app>
						<lem wit="#Transcription">ia mach iz wol armer liute housgenoz.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="59">
					<app>
						<lem wit="#Transcription">doch dinge ich ze liebe,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="60">
					<app>
						<lem wit="#Transcription">chumt mir trost von einem scho&esup;nen wibe.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
		</body>
	</text>
</TEI>