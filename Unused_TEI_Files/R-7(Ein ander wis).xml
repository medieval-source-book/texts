	<text>
		<front>
			<head>
				<title>
					<app>
						<lem wit="#Transcription">Ein ander wis</lem>
						<rdg wit="#Translation">Another song</rdg>
					</app>
				</title>
			</head>
		</front>
		<body>
			<lg type="poem">
				<lg n="1">
					<l n="1">
						<app>
							<lem wit="#Transcription">Nu ist der liebe sumer hin gescheiden</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="2">
						<app>
							<lem wit="#Transcription">die blumen und der voglin sanch</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="3">
						<app>
							<lem wit="#Transcription">muzze wir dem leiden winder lazzen.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="4">
						<app>
							<lem wit="#Transcription">den ungemach</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="5">
						<app>
							<lem wit="#Transcription">meht ein iglich hercze wol von waren schulden chlagen.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="6">
						<app>
							<lem wit="#Transcription">hohgemu&esup;te het ich von in beiden.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="7">
						<app>
							<lem wit="#Transcription">die wile duhte mich so lanch,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="8">
						<app>
							<lem wit="#Transcription">daz si niht ensprungen uf den strazzen.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="9">
						<app>
							<lem wit="#Transcription">min oug ansah,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="10">
						<app>
							<lem wit="#Transcription">daz si giengen alle tag als ein gesmirter wagen,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="11">
						<app>
							<lem wit="#Transcription">eben und leise, niht bedrungen,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="12">
						<app>
							<lem wit="#Transcription">daz in diu swert</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="13">
						<app>
							<lem wit="#Transcription">&uring;f den versen chlungen.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="14">
						<app>
							<lem wit="#Transcription">sih dauhten sumeliche da vil manger b&esup;ne wert.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
				</lg>
				<lg n="2">
					<l n="1">
						<app>
							<lem wit="#Transcription">Die daz waren, des wil ich iuch wisen:</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="2">
						<app>
							<lem wit="#Transcription">da ist Engelvrit und Adeldich,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="3">
						<app>
							<lem wit="#Transcription">Willepreht und Enczeman der iunge</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="4">
						<app>
							<lem wit="#Transcription">und Perewin</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="5">
						<app>
							<lem wit="#Transcription">Sigeloch un Ekkerich und iener Engelram.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="6">
						<app>
							<lem wit="#Transcription">wol gevurpet sint ir chepeleysen</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="7">
						<app>
							<lem wit="#Transcription">ir helcze chlingent nach dem trit</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="8">
						<app>
							<lem wit="#Transcription">loute bei dem reien nah dem sprunge.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="9">
						<app>
							<lem wit="#Transcription">si wellent sin</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="10">
						<app>
							<lem wit="#Transcription">tumber danne der uns Vriderun ir spiegel nam.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="11">
						<app>
							<lem wit="#Transcription">des gewaltes was uns hie zerunnen.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="12">
						<app>
							<lem wit="#Transcription">nu sint ez ien</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="13">
						<app>
							<lem wit="#Transcription">die mir vroud enbunnen</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="14">
						<app>
							<lem wit="#Transcription">un mir die guten verrent, nach der ich min hercze sen.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
				</lg>
				<lg n="3">
					<l n="1">
						<app>
							<lem wit="#Transcription">Der ich her gedienet han von chinde</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="2">
						<app>
							<lem wit="#Transcription">un noch ouch in dem willen bin,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="3">
						<app>
							<lem wit="#Transcription">daz ich wil beliben an ir st&aelig;te</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="4">
						<app>
							<lem wit="#Transcription">vil mangen tach,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="5">
						<app>
							<lem wit="#Transcription">so wol mich, daz ich si ie so minnechlichen vant.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="6">
						<app>
							<lem wit="#Transcription">si ist mins herczen ingesinde,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="7">
						<app>
							<lem wit="#Transcription">diu weile get mir schone hin,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="8">
						<app>
							<lem wit="#Transcription">swenne ich si in wolgetaner w&aelig;te</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="9">
						<app>
							<lem wit="#Transcription">geshen mach,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="10">
						<app>
							<lem wit="#Transcription">so dunch ich mich richer danne ich hiet ein eigen lant.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="11">
						<app>
							<lem wit="#Transcription">ich gesah nie wip so wolgetane,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="12">
						<app>
							<lem wit="#Transcription">des muz ich iehen.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="13">
						<app>
							<lem wit="#Transcription">sunne unt ouch der mane</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="14">
						<app>
							<lem wit="#Transcription">gelichent sich der schonen niht, oder ich enchan niht spehen.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
				</lg>
				<lg n="4">
					<l n="1">
						<app>
							<lem wit="#Transcription">Der mir miner vrowen hulde erwende,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="2">
						<app>
							<lem wit="#Transcription">wizzet daz, wirt mir sin stat,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="3">
						<app>
							<lem wit="#Transcription">daz ich im einen punkelin erzeige,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="4">
						<app>
							<lem wit="#Transcription">als ich hiwer tet</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="5">
						<app>
							<lem wit="#Transcription">einem gouche, der min ouch niht wol hin z'ir gew&uring;ch.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="6">
						<app>
							<lem wit="#Transcription">Fridelip, so we dir in die zende!</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="7">
						<app>
							<lem wit="#Transcription">du bist der gogelheit so sat,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="8">
						<app>
							<lem wit="#Transcription">daz du wil, swar du din houbet geneigest</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="9">
						<app>
							<lem wit="#Transcription">durch mine bet,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="10">
						<app>
							<lem wit="#Transcription">daz dir iemen iht versag. owe, daz ichz vertruch,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="11">
						<app>
							<lem wit="#Transcription">mit Elsemu&esup;t sin uppicheit gereune,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="12">
						<app>
							<lem wit="#Transcription">des er da phlach.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="13">
						<app>
							<lem wit="#Transcription">ir sind leider neune,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="14">
						<app>
							<lem wit="#Transcription">die mir daz geu verpietent mangen liehten veiertach.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
				</lg>
				<lg n="5">
					<l n="1">
						<app>
							<lem wit="#Transcription">Die gehellent alle Perewine</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="2">
						<app>
							<lem wit="#Transcription">wan Enczeman und Willepreht:</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="3">
						<app>
							<lem wit="#Transcription">die enziehent mit in niht geleiche,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="4">
						<app>
							<lem wit="#Transcription">nu sprichet er,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="5">
						<app>
							<lem wit="#Transcription">ist daz ers ersnellen meg, si sin bede tot,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="6">
						<app>
							<lem wit="#Transcription">er slachs, daz diu sunne durch si schiene.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="7">
						<app>
							<lem wit="#Transcription">si rouften sines vater chneht</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="8">
						<app>
							<lem wit="#Transcription">hiwer vor dem mayer Frideriche</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="9">
						<app>
							<lem wit="#Transcription">umbe nie neht me</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="10">
						<app>
							<lem wit="#Transcription">wan daz er ein chrenczel truch, daz was von bluomen rot.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="11">
						<app>
							<lem wit="#Transcription">daz verseit er da zehant den meyden.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="12">
						<app>
							<lem wit="#Transcription">nu wizzet daz,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="13">
						<app>
							<lem wit="#Transcription">wirt ez niht gescheiden,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="14">
						<app>
							<lem wit="#Transcription">ez w&aelig;hset lihte zwischen in ein ungevuger haz.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
				</lg>
				<lg n="6">
					<l n="1">
						<app>
							<lem wit="#Transcription">Daz die dorper alle an ander slugen,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="2">
						<app>
							<lem wit="#Transcription">daz liezze ich also hine gan,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="3">
						<app>
							<lem wit="#Transcription">wan si tunt mir vil ze widerdriezze</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="4">
						<app>
							<lem wit="#Transcription">ir uppicheit</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="5">
						<app>
							<lem wit="#Transcription">diu ist so groz, daz ir die wisen spottent uberal.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="6">
						<app>
							<lem wit="#Transcription">daz sich doch vil lihte mach gevuegen.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="7">
						<app>
							<lem wit="#Transcription">wer ot er sich, Enczeman!</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="8">
						<app>
							<lem wit="#Transcription">triffet er mit sienem scharfen spiezze,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="9">
						<app>
							<lem wit="#Transcription">den er da treit,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="10">
						<app>
							<lem wit="#Transcription">er gedranget mich niht mer da ze Riwental.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="11">
						<app>
							<lem wit="#Transcription">ich bin vreuden gar von in versumet,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="12">
						<app>
							<lem wit="#Transcription">daz ist niht gu&esup;t.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="13">
						<app>
							<lem wit="#Transcription">wrde mir geroumet</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="14">
						<app>
							<lem wit="#Transcription">von in, daz muzze wir verchlagen, ich und Elsemu&esup;t.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
				</lg>
				<lg n="7">
					<l n="1">
						<app>
							<lem wit="#Transcription">"Laufet, lieber &aelig;tte, helfet scheiden!</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="2">
						<app>
							<lem wit="#Transcription">sie veustent in der studen swert.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="3">
						<app>
							<lem wit="#Transcription">daz hat aber Enczeman gemachet.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="4">
						<app>
							<lem wit="#Transcription">der wil des niht,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="5">
						<app>
							<lem wit="#Transcription">daz D&aelig;gen oder Ucze bei Elsemute ge.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="6">
						<app>
							<lem wit="#Transcription">..........</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="7">
						<app>
							<lem wit="#Transcription">..........</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="8">
						<app>
							<lem wit="#Transcription">..........</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="9">
						<app>
							<lem wit="#Transcription">..........</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="10">
						<app>
							<lem wit="#Transcription">..........</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="11">
						<app>
							<lem wit="#Transcription">ir gahet bald e s'im daz hutel zerren.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="12">
						<app>
							<lem wit="#Transcription">chomt schiere dar,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="13">
						<app>
							<lem wit="#Transcription">oder si slahent im ein vlerren</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="14">
						<app>
							<lem wit="#Transcription">durch daz wange, seht, wie greulich im daz ste!"</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
				</lg>
			</lg>
		</body>
	</text>
</TEI>

