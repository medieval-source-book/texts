		<body>
			<lg type="stanza" n="1">
				<l n="1">
					<app>
						<lem wit="#Transcription">Owe, sumerwunne,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="2">
					<app>
						<lem wit="#Transcription">daz ich mich din anen mu&esup;z</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="3">
					<app>
						<lem wit="#Transcription">- der mir din enbunne,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="4">
					<app>
						<lem wit="#Transcription">dem enwede nimmer bu&esup;z</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="5">
					<app>
						<lem wit="#Transcription">herczenlicher leide -</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="6">
					<app>
						<lem wit="#Transcription">unt der wolgetanen,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="7">
					<app>
						<lem wit="#Transcription">noch der ie min hercze ranch!</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="8">
					<app>
						<lem wit="#Transcription">sol ich mich ir anen,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="9">
					<app>
						<lem wit="#Transcription">daz ist under minen danch.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="10">
					<app>
						<lem wit="#Transcription">swenn ich von ir scheide,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="11">
					<app>
						<lem wit="#Transcription">so geschiet nie man unsanfter von einem wibe.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="12">
					<app>
						<lem wit="#Transcription">bezzer wære mir der tot</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="13">
					<app>
						<lem wit="#Transcription">dann ein sælichlichiu not</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="14">
					<app>
						<lem wit="#Transcription">lange also belibe.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="2">
				<l n="15">
					<app>
						<lem wit="#Transcription">Chlagt ich nu besunder</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="16">
					<app>
						<lem wit="#Transcription">waz ich leides ie gewan,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="17">
					<app>
						<lem wit="#Transcription">ich han ez vur wunder,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="18">
					<app>
						<lem wit="#Transcription">daz mir maniger miht engan,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="19">
					<app>
						<lem wit="#Transcription">ob mir liep geschæhe</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="20">
					<app>
						<lem wit="#Transcription">von dem besten wibe,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="21">
					<app>
						<lem wit="#Transcription">di ich mit ougen ie gesah.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="22">
					<app>
						<lem wit="#Transcription">si hat an ir libe</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="23">
					<app>
						<lem wit="#Transcription">des man ir ze gu&esup;te iah.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="24">
					<app>
						<lem wit="#Transcription">swie si mich versmæhe,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="25">
					<app>
						<lem wit="#Transcription">ich geloube niht, daz sis also von herczen mæine.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="26">
					<app>
						<lem wit="#Transcription">ich getrow ir als ich sol</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="27">
					<app>
						<lem wit="#Transcription">lones und genagen wol.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="28">
					<app>
						<lem wit="#Transcription">half ab ez mich chlæine!</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="3">
				<l n="29">
					<app>
						<lem wit="#Transcription">Ich bin in von schulden</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="30">
					<app>
						<lem wit="#Transcription">immer neidich unde gram,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="31">
					<app>
						<lem wit="#Transcription">di mich von ir hulden</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="32">
					<app>
						<lem wit="#Transcription">dwingent. daz ist Berhtram</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="33">
					<app>
						<lem wit="#Transcription">und der junge Gozze</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="34">
					<app>
						<lem wit="#Transcription">und der ungenande,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="35">
					<app>
						<lem wit="#Transcription">des ich minnen niht entar,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="36">
					<app>
						<lem wit="#Transcription">der daz gerne wande,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="37">
					<app>
						<lem wit="#Transcription">næme si min inder war.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="38">
					<app>
						<lem wit="#Transcription">sine spiezgenozzen,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="39">
					<app>
						<lem wit="#Transcription">der swaimet einer von dem obsertem pyrboume,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="40">
					<app>
						<lem wit="#Transcription">der si um ir minne bat.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="41">
					<app>
						<lem wit="#Transcription">u&osup;f daz rochel er ir trat</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="42">
					<app>
						<lem wit="#Transcription">da niden bei dem soume.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="4">
				<l n="43">
					<app>
						<lem wit="#Transcription">Da si bi dem tanze</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="44">
					<app>
						<lem wit="#Transcription">gie, er gie ir an der hant.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="45">
					<app>
						<lem wit="#Transcription">von dem ridewancze</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="46">
					<app>
						<lem wit="#Transcription">chom sin vuez u&osup;f ur gewant,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="47">
					<app>
						<lem wit="#Transcription">daz lag an der erde</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="48">
					<app>
						<lem wit="#Transcription">von dem umbesliffen,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="49">
					<app>
						<lem wit="#Transcription">daz dem iunge sanfte tu&esup;t,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="50">
					<app>
						<lem wit="#Transcription">ward er von der pheiffe</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="51">
					<app>
						<lem wit="#Transcription">uppich und hohgemu&esup;t</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="52">
					<app>
						<lem wit="#Transcription">wand er gie im werde.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="53">
					<app>
						<lem wit="#Transcription">sælten chom sin munt mit rounen danches u&osup;z ir oren,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="54">
					<app>
						<lem wit="#Transcription">des vil sere mich verdroz.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="55">
					<app>
						<lem wit="#Transcription">[.........................]</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="56">
					<app>
						<lem wit="#Transcription">[..........] guter sinne toren.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="5">
				<l n="57">
					<app>
						<lem wit="#Transcription">Von der Persenich</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="58">
					<app>
						<lem wit="#Transcription">wider uncz an daz Ungertor</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="59">
					<app>
						<lem wit="#Transcription">in der dorper diche</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="60">
					<app>
						<lem wit="#Transcription">weiz ich ninder zwene vor,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="61">
					<app>
						<lem wit="#Transcription">di mit ebenheuze</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="62">
					<app>
						<lem wit="#Transcription">sich zu zin gelichen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="63">
					<app>
						<lem wit="#Transcription">ia, wæn, inder zwene chnaben</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="64">
					<app>
						<lem wit="#Transcription">in allen deutschen richen</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="65">
					<app>
						<lem wit="#Transcription">bezzer mit wiben haben</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="66">
					<app>
						<lem wit="#Transcription">niht gein einer grouze.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="67">
					<app>
						<lem wit="#Transcription">Engelmar gewan ez niht so gu&esup;t mit Vrideroune,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="68">
					<app>
						<lem wit="#Transcription">als ez doch der eine hat.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="69">
					<app>
						<lem wit="#Transcription">+iener durchel er ir diu wat,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="70">
					<app>
						<lem wit="#Transcription">e daz er geroune.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
		</body>
