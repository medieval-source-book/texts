	<text>
		<front>
			<head>
				<title>
					<app>
						<lem wit="#Transcription">Ein ander</lem>
						<rdg wit="#Translation"><!--TITLE'S TRANSLATION--></rdg>
					</app>
				</title>
			</head>
		</front>
		<body>
			<lg type="stanza" n="1">
				<l n="1">
					<app>
						<lem wit="#Transcription">"Schowet an den walt, wi er niwes loubes reichet,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="2">
					<app>
						<lem wit="#Transcription">wie wol er siniu gruniu chleider an sich streichet.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="3">
					<app>
						<lem wit="#Transcription">der hat im der maye</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="4">
					<app>
						<lem wit="#Transcription">vil gesant.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="5">
					<app>
						<lem wit="#Transcription">maget, so man reie,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="6">
					<app>
						<lem wit="#Transcription">so sit gemant</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="7">
					<app>
						<lem wit="#Transcription">alle,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="8">
					<app>
						<lem wit="#Transcription">daz wir di rosenchrenczel</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="9">
					<app>
						<lem wit="#Transcription">brechen,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="10">
					<app>
						<lem wit="#Transcription">so daz tau daran gevalle.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="2">
				<l n="11">
					<app>
						<lem wit="#Transcription">Hey sumer, waz herczen gein diner chunft erlachet!</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="12">
					<app>
						<lem wit="#Transcription">di vogel die der winder trourich het gemachet,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="13">
					<app>
						<lem wit="#Transcription">di singent wunnichlichen</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="14">
					<app>
						<lem wit="#Transcription">ir gesanch,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="15">
					<app>
						<lem wit="#Transcription">wellent in aber teichen</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="16">
					<app>
						<lem wit="#Transcription">den sumer lanch.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="17">
					<app>
						<lem wit="#Transcription">schalles</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="18">
					<app>
						<lem wit="#Transcription">phlegent si des morgens,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="19">
					<app>
						<lem wit="#Transcription">gein abent</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="20">
					<app>
						<lem wit="#Transcription">spil wir chint des palles.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="3">
				<l n="21">
					<app>
						<lem wit="#Transcription">Vreud und churczwile sul wir uns hiwer nieten.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="22">
					<app>
						<lem wit="#Transcription">got sol den iungen magden allen daz gebieten,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="23">
					<app>
						<lem wit="#Transcription">daz si mit lihter wæte</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="24">
					<app>
						<lem wit="#Transcription">sin bereit</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="25">
					<app>
						<lem wit="#Transcription">und den sumer stæt</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="26">
					<app>
						<lem wit="#Transcription">an ir hobescheit.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="27">
					<app>
						<lem wit="#Transcription">winder</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="28">
					<app>
						<lem wit="#Transcription">hat ez hie geroumet.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="29">
					<app>
						<lem wit="#Transcription">die alten</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="30">
					<app>
						<lem wit="#Transcription">suln sin desder chinder.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="4">
				<l n="31">
					<app>
						<lem wit="#Transcription">Als ich di sumerwunne bi der vogel reide erchenne,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="32">
					<app>
						<lem wit="#Transcription">di blumen di der maye lost ouz reiffen bande</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="33">
					<app>
						<lem wit="#Transcription">mit sinem liehten schine</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="34">
					<app>
						<lem wit="#Transcription">wolgetan,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="35">
					<app>
						<lem wit="#Transcription">het ich Jeuteline,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="36">
					<app>
						<lem wit="#Transcription">so wold ich gan</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="37">
					<app>
						<lem wit="#Transcription">schowen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="38">
					<app>
						<lem wit="#Transcription">diu lind ist wol bevangen</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="39">
					<app>
						<lem wit="#Transcription">mit loube.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="40">
					<app>
						<lem wit="#Transcription">darunder tanzent vrowen."</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="5">
				<l n="41">
					<app>
						<lem wit="#Transcription">"Da wil ich din hutten", sprach des chindes æide.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="42">
					<app>
						<lem wit="#Transcription">"nu ge wir mit ein ander zu der linden beide.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="43">
					<app>
						<lem wit="#Transcription">ich pin miner iare</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="44">
					<app>
						<lem wit="#Transcription">gar ein chint,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="45">
					<app>
						<lem wit="#Transcription">wan daz minem hare</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="46">
					<app>
						<lem wit="#Transcription">di loche sint</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="47">
					<app>
						<lem wit="#Transcription">greise.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="48">
					<app>
						<lem wit="#Transcription">di wil ich bewinden</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="49">
					<app>
						<lem wit="#Transcription">mit siden.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="50">
					<app>
						<lem wit="#Transcription">tohter, wo ist min rise?"</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="6">
				<l n="51">
					<app>
						<lem wit="#Transcription">"Muter, di reisen han ich vor iu behalten,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="52">
					<app>
						<lem wit="#Transcription">diu zimt einer iungen baz dann einer alten</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="53">
					<app>
						<lem wit="#Transcription">ze tragen umb ir houbet</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="54">
					<app>
						<lem wit="#Transcription">an der schar.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="55">
					<app>
						<lem wit="#Transcription">wer hat iuch berowet</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="56">
					<app>
						<lem wit="#Transcription">der sinne gar?</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="57">
					<app>
						<lem wit="#Transcription">slaffet!</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="58">
					<app>
						<lem wit="#Transcription">waz ob u nu ringer</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="59">
					<app>
						<lem wit="#Transcription">getroumet,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="60">
					<app>
						<lem wit="#Transcription">daz ir iuch anders zaffet?"</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="7">
				<l n="61">
					<app>
						<lem wit="#Transcription">Wie si den strit liezzen, daz wil ich iu bescheiden.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="62">
					<app>
						<lem wit="#Transcription">daz magedin begunde siner muter leiden.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="63">
					<app>
						<lem wit="#Transcription">zwene roten golzen</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="64">
					<app>
						<lem wit="#Transcription">si verstal</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="65">
					<app>
						<lem wit="#Transcription">einem ritter stolczen</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="66">
					<app>
						<lem wit="#Transcription">von Riwental</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="67">
					<app>
						<lem wit="#Transcription">tougen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="68">
					<app>
						<lem wit="#Transcription">si bot im bi dem tanze</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="69">
					<app>
						<lem wit="#Transcription">ein chrenczel,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="70">
					<app>
						<lem wit="#Transcription">so mir got, daz ist unlougen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
		</body>
	</text>
</TEI>