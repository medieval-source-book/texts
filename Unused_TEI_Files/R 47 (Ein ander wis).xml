	<text>
		<front>
			<head>
				<title>
					<app>
						<lem wit="#Transcription">Ein ander wis</lem>
						<rdg wit="#Translation"><!--TITLE'S TRANSLATION--></rdg>
					</app>
				</title>
			</head>
		</front>
		<body>
			<lg type="stanza" n="1">
				<l n="1">
					<app>
						<lem wit="#Transcription">Owe dirre sumerzit,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="2">
					<app>
						<lem wit="#Transcription">owe blumen unde chle,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="3">
					<app>
						<lem wit="#Transcription">owe maniger wunne, der wir ane muzzen sin!</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="4">
					<app>
						<lem wit="#Transcription">unser beider widerstreit</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="5">
					<app>
						<lem wit="#Transcription">bringet riffen und sne,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="6">
					<app>
						<lem wit="#Transcription">daz hat als rosen roten ungelichen schin.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="7">
					<app>
						<lem wit="#Transcription">also ist ungelich</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="8">
					<app>
						<lem wit="#Transcription">min und Amelunges swære.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="9">
					<app>
						<lem wit="#Transcription">mines ungelingen vreut er sich und Udelrich.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="10">
					<app>
						<lem wit="#Transcription">der ist mines schaden zallen ziten vlizzich und geværich,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="11">
					<app>
						<lem wit="#Transcription">er und Eberolt, ein ungestumer weterich.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="2">
				<l n="12">
					<app>
						<lem wit="#Transcription">Eberolt und Amlunch,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="13">
					<app>
						<lem wit="#Transcription">Udelger und Undelhart</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="14">
					<app>
						<lem wit="#Transcription">habent wider mich gebruvet ein sicherheit.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="15">
					<app>
						<lem wit="#Transcription">manich &oelig;dechlicher sprunch</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="16">
					<app>
						<lem wit="#Transcription">von in do gesprungen wart,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="17">
					<app>
						<lem wit="#Transcription">do si sich des ruemten, si getæten mir ein leit.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="18">
					<app>
						<lem wit="#Transcription">still und offenbar</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="19">
					<app>
						<lem wit="#Transcription">habent si den ruem behert.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="20">
					<app>
						<lem wit="#Transcription">ich wunsch in nimmer, daz ir cheiner wol gevar.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="21">
					<app>
						<lem wit="#Transcription">under disen vieren hat mir einer den minen mut beswæret,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="22">
					<app>
						<lem wit="#Transcription">daz er nie so truebe wart von iu, her Engelmar.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="3">
				<l n="23">
					<app>
						<lem wit="#Transcription">Wesse ich, wem ich solde chlagen</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="24">
					<app>
						<lem wit="#Transcription">minen grozzen ungemach,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="25">
					<app>
						<lem wit="#Transcription">den ich von in leide und lange her geliten han!</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="26">
					<app>
						<lem wit="#Transcription">swaz mir noch bi minen tagen</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="27">
					<app>
						<lem wit="#Transcription">leides ie von in geschach,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="28">
					<app>
						<lem wit="#Transcription">daz ist ein wint wan daz mir nu der eine hat getan.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="29">
					<app>
						<lem wit="#Transcription">owe, daz ich sol</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="30">
					<app>
						<lem wit="#Transcription">nu min sælbes laster ruegen!</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="31">
					<app>
						<lem wit="#Transcription">miner ougen wunne greif er an den fudenol.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="32">
					<app>
						<lem wit="#Transcription">tumber gouch, des mehte den cheiser Friderichen wol genugen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="33">
					<app>
						<lem wit="#Transcription">honer schimph gevellet nimmer gu&esup;ten liuten wol.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="4">
				<l n="34">
					<app>
						<lem wit="#Transcription">Iz ist vrowen e geschen</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="35">
					<app>
						<lem wit="#Transcription">an ir willen sunder danch,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="36">
					<app>
						<lem wit="#Transcription">daz der lieben und der wolgetanen da geschach.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="37">
					<app>
						<lem wit="#Transcription">hiet si den grif gesehen,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="38">
					<app>
						<lem wit="#Transcription">si ist ir libes nie so chranch,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="39">
					<app>
						<lem wit="#Transcription">er hiet sin buez enphangen, des si sit veriach.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="40">
					<app>
						<lem wit="#Transcription">snæller dann ein bolcz</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="41">
					<app>
						<lem wit="#Transcription">was sin leip, ir leit ergangen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="42">
					<app>
						<lem wit="#Transcription">immer mer was der dorper sines herczen stolcz.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="43">
					<app>
						<lem wit="#Transcription">done chunde er an den stunden niht vollangen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="44">
					<app>
						<lem wit="#Transcription">di unwæge richt uns bei den herren chnutelholcz!</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="5">
				<l n="45">
					<app>
						<lem wit="#Transcription">Mine vriunt, nu get her dan,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="46">
					<app>
						<lem wit="#Transcription">gebt mir iwern wisen rat,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="47">
					<app>
						<lem wit="#Transcription">wie ich mit disen dingen zu minen eren muge chomen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="48">
					<app>
						<lem wit="#Transcription">aller triwen ich iuch man,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="49">
					<app>
						<lem wit="#Transcription">daz ir mir nu bei gestat.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="50">
					<app>
						<lem wit="#Transcription">mine waidegenge und elliu min vreud ist mir benomen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="51">
					<app>
						<lem wit="#Transcription">ich bin unverzeit</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="52">
					<app>
						<lem wit="#Transcription">beidiu an lip und ouch an mu&esup;te.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="53">
					<app>
						<lem wit="#Transcription">der in durch den willen min sin dinest widersagte,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="54">
					<app>
						<lem wit="#Transcription">dem gestund ich immer triwen bi mit leib und ouch mit gute</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="55">
					<app>
						<lem wit="#Transcription">alle di weil und mir der stegereif ze hove wagete.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
		</body>
	</text>
</TEI>