	<text>
		<front>
			<div>
				<listWit>
					<witness xml:id="Transcription">Ein ander weis</witness>
					<witness xml:id="Translation"><!--TITLE'S TRANSLATION--></witness>
				</listWit>
			</div>
		</front>
		<body>
			<head>
				<title>
					<app>
						<lem wit="#Transcription">Ein ander weis</lem>
						<rdg wit="#Translation"><!--TITLE'S TRANSLATION--></rdg>
					</app>
				</title>
			</head>
			<lg type=stanza n="1">
				<l n="1">
					<app>
						<lem wit="#Transcription">Mir ist von herczen leide,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="2">
					<app>
						<lem wit="#Transcription">daz der chu&esup;le winder</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="3">
					<app>
						<lem wit="#Transcription">verterbet schone blu&osup;men vil:</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="4">
					<app>
						<lem wit="#Transcription">so verternet mich ein senelichiu arbeit.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="5">
					<app>
						<lem wit="#Transcription">dise sorge beide</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="6">
					<app>
						<lem wit="#Transcription">dringent mich hin hinder</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="7">
					<app>
						<lem wit="#Transcription">z'ende an miner vreiden zil.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="8">
					<app>
						<lem wit="#Transcription">owe, daz diu gu&esup;te mit ir willen daz vertreit,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="9">
					<app>
						<lem wit="#Transcription">seit si wol geringen mach</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="10">
					<app>
						<lem wit="#Transcription">alle mine sw&aelig;re.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="11">
					<app>
						<lem wit="#Transcription">owe, gelebt ich noch den tach,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="12">
					<app>
						<lem wit="#Transcription">daz si gen&aelig;dich w&aelig;re!</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="2">
				<l n="13">
					<app>
						<lem wit="#Transcription">Swenne ich mich vereine</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="14">
					<app>
						<lem wit="#Transcription">un  an si gedenche,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="15">
					<app>
						<lem wit="#Transcription">w&aelig;r inder wibes gu&esup;te da,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="16">
					<app>
						<lem wit="#Transcription">diu n&aelig;hete sich so lange bei ir niht verholn.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="17">
					<app>
						<lem wit="#Transcription">sit si lonet chleine</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="18">
					<app>
						<lem wit="#Transcription">miner niwen chlenche,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="19">
					<app>
						<lem wit="#Transcription">mag ich dienen anderswa?</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="20">
					<app>
						<lem wit="#Transcription">nein, ich wil mit willen disen chumber langer doln.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="21">
					<app>
						<lem wit="#Transcription">waz ob nich ein s&aelig;lich wip</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="22">
					<app>
						<lem wit="#Transcription">gar den mu&esup;t vercheret</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="23">
					<app>
						<lem wit="#Transcription">un vreunt min hercz un  ouch den lip?</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="24">
					<app>
						<lem wit="#Transcription">diu zwei die sint geseret.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="3">
				<l n="25">
					<app>
						<lem wit="#Transcription">Zu dem ungemache,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="26">
					<app>
						<lem wit="#Transcription">den uch von ir leide,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="27">
					<app>
						<lem wit="#Transcription">so twinget mich ein ander leit,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="28">
					<app>
						<lem wit="#Transcription">daz vor allem leide mich so sere ie betwanch,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="29">
					<app>
						<lem wit="#Transcription">swie ich dar umbe lache</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="30">
					<app>
						<lem wit="#Transcription">un gebar so bleide.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="31">
					<app>
						<lem wit="#Transcription">mir hat ein do&esup;rper widerseit</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="32">
					<app>
						<lem wit="#Transcription">umbe anders niht wan umbe den minen uppechlichen sanch.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="33">
					<app>
						<lem wit="#Transcription">der ist geheizzen Adeltyer,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="34">
					<app>
						<lem wit="#Transcription">burtich her von Ense.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="35">
					<app>
						<lem wit="#Transcription">zallen ziten drot er mir</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="36">
					<app>
						<lem wit="#Transcription">als einer veizten gense.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="4">
				<l n="37">
					<app>
						<lem wit="#Transcription">Hiwer an einem tancze</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="38">
					<app>
						<lem wit="#Transcription">gie er umb und umbe.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="39">
					<app>
						<lem wit="#Transcription">den whsel het er al den tach:</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="40">
					<app>
						<lem wit="#Transcription">glancziu schapel gap er umb ir niwen chrenzelinch.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="41">
					<app>
						<lem wit="#Transcription">Eczel un Lancze,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="43">
					<app>
						<lem wit="#Transcription">czwnen chnappen tumbe,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="44">
					<app>
						<lem wit="#Transcription">die plagen ouch des iener phlach.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="45">
					<app>
						<lem wit="#Transcription">Lancze dar besw&aelig;ret ein vil stolczez magedin.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="46">
					<app>
						<lem wit="#Transcription">eine chleine risen gu&oring;t</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="47">
					<app>
						<lem wit="#Transcription">zart er ab ir houbet,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="48">
					<app>
						<lem wit="#Transcription">daz zu einen blu&oring;menh&uring;t.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="49">
					<app>
						<lem wit="#Transcription">dar zu einen blu&oring;menh&uring;t.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="50">
					<app>
						<lem wit="#Transcription">wer het im daz erloubet?</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="5">
				<l n="51">
					<app>
						<lem wit="#Transcription">Owe siner hende,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="52">
					<app>
						<lem wit="#Transcription">daz si sin verwazzen!</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="53">
					<app>
						<lem wit="#Transcription">die vinfer muzzen vlorn,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="54">
					<app>
						<lem wit="#Transcription">da mir er geserret hat den schendelichen zar!</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="55">
					<app>
						<lem wit="#Transcription">hiet er ir gebende</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="56">
					<app>
						<lem wit="#Transcription">ungezerret lazzen,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="57">
					<app>
						<lem wit="#Transcription">daz chr&aelig;ntel hit ouch si verchorn.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="58">
					<app>
						<lem wit="#Transcription">er ist ungevu&esup;ger danne wilen Engelmar,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="59">
					<app>
						<lem wit="#Transcription">der gewaltichlichen nam</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="60">
					<app>
						<lem wit="#Transcription">den spiegel Vrideronen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="61">
					<app>
						<lem wit="#Transcription">des bin ich dem dorper gram,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="62">
					<app>
						<lem wit="#Transcription">dem selben Walberounen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="6">
				<l n="63">
					<app>
						<lem wit="#Transcription">Die min vil alten schulde</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="64">
					<app>
						<lem wit="#Transcription">wechet mir die niwe:</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="65">
					<app>
						<lem wit="#Transcription">ez hat ein getelinch</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="66">
					<app>
						<lem wit="#Transcription">hiwer an mir erwechet, swaz mit leides ie geschah.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="67">
					<app>
						<lem wit="#Transcription">e ichz langer dulte,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="68">
					<app>
						<lem wit="#Transcription">set des min triwe,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="69">
					<app>
						<lem wit="#Transcription">gespring ich zu zim in den rinch.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="70">
					<app>
						<lem wit="#Transcription">er bestat sin buzze, daz er ir ze crowen iach,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="71">
					<app>
						<lem wit="#Transcription">der ich lange gedienet han</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="72">
					<app>
						<lem wit="#Transcription">her mit ganczer st&aelig;te.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="73">
					<app>
						<lem wit="#Transcription">wold er si geruet an,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="74">
					<app>
						<lem wit="#Transcription">wie reht er danne t&aelig;te!</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="7">
				<l n="75">
					<app>
						<lem wit="#Transcription">We waz hat er muechen!</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="76">
					<app>
						<lem wit="#Transcription">si chumpt im niht ze mazze.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="77">
					<app>
						<lem wit="#Transcription">zwiu sol sin oeinechlich gebrech?</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="78">
					<app>
						<lem wit="#Transcription">im enmach gehelfen niht sin hovelich gewant.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="79">
					<app>
						<lem wit="#Transcription">er sol im eine su&esup;chen,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="80">
					<app>
						<lem wit="#Transcription">diu in werben lazze.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="81">
					<app>
						<lem wit="#Transcription">diu sinen roten busemblech,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="82">
					<app>
						<lem wit="#Transcription">diu sint ir ungen&aelig;me, dar zu sin huffelbant.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="83">
					<app>
						<lem wit="#Transcription">enge ermel treit er lanch,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="84">
					<app>
						<lem wit="#Transcription">die sint vor gebr&aeling;met,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="85">
					<app>
						<lem wit="#Transcription">innen swartz un euzen blanch.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="86">
					<app>
						<lem wit="#Transcription">mit siner red er vl&aeling;met.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
		</body>
	</text>
