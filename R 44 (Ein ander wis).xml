	<text>
		<front>
			<head>
				<title>
					<app>
						<lem wit="#Transcription">Ein ander wis</lem>
						<rdg wit="#Translation"><!--TITLE'S TRANSLATION--></rdg>
					</app>
				</title>
			</head>
		</front>
		<body>
			<lg type="stanza" n="1">
				<l n="1">
					<app>
						<lem wit="#Transcription">Owe dirre not!</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="2">
					<app>
						<lem wit="#Transcription">wie habent sich verwandelot</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="3">
					<app>
						<lem wit="#Transcription">dise liehten sumertage!</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="4">
					<app>
						<lem wit="#Transcription">von so senelicher chlage</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="5">
					<app>
						<lem wit="#Transcription">trouret manich hercze, daz in hohem mute was.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="6">
					<app>
						<lem wit="#Transcription">daz ist aber elliu iar,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="7">
					<app>
						<lem wit="#Transcription">daz der winder offenbar,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="8">
					<app>
						<lem wit="#Transcription">uns beroubet ane wer</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="9">
					<app>
						<lem wit="#Transcription">mit getwaltichlichem her.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="10">
					<app>
						<lem wit="#Transcription">er benimt uns vil der schonen blumen unde gras.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="11">
					<app>
						<lem wit="#Transcription">also hat ein wip</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="12">
					<app>
						<lem wit="#Transcription">mich beroubet gar der sinne,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="13">
					<app>
						<lem wit="#Transcription">an den triwen, und ich si so herczenlichen minne.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="14">
					<app>
						<lem wit="#Transcription">wie ward ungænedich ie so minnichlicher lip?</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="2">
				<l n="15">
					<app>
						<lem wit="#Transcription">Ich pin zewaier schaden</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="16">
					<app>
						<lem wit="#Transcription">von ir schulden uberladen,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="17">
					<app>
						<lem wit="#Transcription">di mir al ze swære sint.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="18">
					<app>
						<lem wit="#Transcription">ich pin tumber dann ein chint,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="19">
					<app>
						<lem wit="#Transcription">daz ich han gedinet ane lon und ane danch.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="20">
					<app>
						<lem wit="#Transcription">so ist daz der dritte schad,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="21">
					<app>
						<lem wit="#Transcription">sæhe si mich u&osup;f dem rad,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="22">
					<app>
						<lem wit="#Transcription">si gespræch nimmer ach,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="23">
					<app>
						<lem wit="#Transcription">des si sælbe mir veriach.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="24">
					<app>
						<lem wit="#Transcription">owe daz ir lop von minem munde ie sus erchlach!</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="25">
					<app>
						<lem wit="#Transcription">si tut als der stein,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="26">
					<app>
						<lem wit="#Transcription">der daz eysen an sich ziuhet.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="27">
					<app>
						<lem wit="#Transcription">von siner grozzen chraft man in mit scheffen sere vliuhet.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="28">
					<app>
						<lem wit="#Transcription">also ziuhet si mich zu ir im gelichem ein.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="3">
				<l n="29">
					<app>
						<lem wit="#Transcription">Wa nu vriundes rat,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="30">
					<app>
						<lem wit="#Transcription">sit si niht genaden hat?</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="31">
					<app>
						<lem wit="#Transcription">wie ich mit disem dinge tu&esup;,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="32">
					<app>
						<lem wit="#Transcription">da bedorf ich rates zu&esup;.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="33">
					<app>
						<lem wit="#Transcription">rat ein igelich vriunt also di rede wære sin.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="34">
					<app>
						<lem wit="#Transcription">schid ich nu von ir,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="35">
					<app>
						<lem wit="#Transcription">seid ich herzenliche gir</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="36">
					<app>
						<lem wit="#Transcription">nah ir werde minne han,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="37">
					<app>
						<lem wit="#Transcription">daz enist niht gu&esup;t getan.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="38">
					<app>
						<lem wit="#Transcription">we warumbe liezz ich nu den langen dinst min?</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="39">
					<app>
						<lem wit="#Transcription">ich wil vurbaz</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="40">
					<app>
						<lem wit="#Transcription">min geluche noch versuchen,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="41">
					<app>
						<lem wit="#Transcription">ob da vrowe Sælde mines heiles welle ruchen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="42">
					<app>
						<lem wit="#Transcription">mir hat hiwer ein getelinch geniwet minen haz.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="4">
				<l n="43">
					<app>
						<lem wit="#Transcription">Daz ist Irnper,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="44">
					<app>
						<lem wit="#Transcription">vert von Potenbrunne her</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="45">
					<app>
						<lem wit="#Transcription">durch sin hofschen da herab,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="46">
					<app>
						<lem wit="#Transcription">ein vil heuzer dorfchnab.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="47">
					<app>
						<lem wit="#Transcription">guter wibe muz im nimmer werden teil.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="48">
					<app>
						<lem wit="#Transcription">daz ist swinder vluch.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="49">
					<app>
						<lem wit="#Transcription">ine chund ez an ein buch</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="50">
					<app>
						<lem wit="#Transcription">nimmer halbez han geschriben,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="51">
					<app>
						<lem wit="#Transcription">daz er wunders hat getriben</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="52">
					<app>
						<lem wit="#Transcription">hiwer mit der lieben, da die iungen waren gæil.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="53">
					<app>
						<lem wit="#Transcription">ob er sich ertobet</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="54">
					<app>
						<lem wit="#Transcription">nah ir minne und erwunne,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="55">
					<app>
						<lem wit="#Transcription">er ist ir ungewert. nu hosch er hin gein Potenbrunne!</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="56">
					<app>
						<lem wit="#Transcription">si hat mich und in und all unstæte man verlobet.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="5">
				<l n="57">
					<app>
						<lem wit="#Transcription">Bræche si den æit,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="58">
					<app>
						<lem wit="#Transcription">liezz ir mine sicherheit</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="59">
					<app>
						<lem wit="#Transcription">von ir vriunden hohe staben,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="60">
					<app>
						<lem wit="#Transcription">daz ichs immer wolde haben</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="61">
					<app>
						<lem wit="#Transcription">liep vor allem liebe hin do liep ein ende hat,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="62">
					<app>
						<lem wit="#Transcription">mæht iz ir gezemen,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="63">
					<app>
						<lem wit="#Transcription">daz siz also wolde nemen,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="64">
					<app>
						<lem wit="#Transcription">als ich ir geteilet han,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="65">
					<app>
						<lem wit="#Transcription">so hiet al min lieber wan</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="66">
					<app>
						<lem wit="#Transcription">sich nah minem willen wol volendet. nune lat</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="67">
					<app>
						<lem wit="#Transcription">iener Irnper</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="68">
					<app>
						<lem wit="#Transcription">mir niht wol an ir gelingen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="69">
					<app>
						<lem wit="#Transcription">iane wil ich nimmer mer wibes lop gesingen,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="70">
					<app>
						<lem wit="#Transcription">ob si mich verzeihet und ir minne iene wer.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
		</body>
	</text>
</TEI>