					</app>
				</l>
				<l n="55">
					<app>
						<lem wit="#Transcription">&uring;f minnen long</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="56">
					<app>
						<lem wit="#Transcription">her mit mir behalten,</lem>
