					</app>
				</l>
				<l n="57">
					<app>
						<lem wit="#Transcription">des tousent hercz wrden geil.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="58">
					<app>
						<lem wit="#Transcription">gewinn ich heil</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="59">
					<app>
						<lem wit="#Transcription">gegen der wolgetanen, min gewerft sol heiles walten.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="9">
				<l n="60">
					<app>
						<lem wit="#Transcription">Si reyen oder tanzen,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="61">
					<app>
						<lem wit="#Transcription">si tun vil maigen weiten schrit,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="62">
					<app>
						<lem wit="#Transcription">ich allez mit.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="63">
					<app>
						<lem wit="#Transcription">Ãª wir heim geswanczen,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="64">
					<app>
						<lem wit="#Transcription">ich sag iz bei den triwen min,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="65">
					<app>
						<lem wit="#Transcription">wir solden sin</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="66">
					<app>
						<lem wit="#Transcription">ze &OElig;sterich: vor dem snit so seczet man di phlanczen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="10">
				<l n="67">
					<app>
						<lem wit="#Transcription">Er dunchet mich ein narre,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="68">
					<app>
						<lem wit="#Transcription">swer disen ougest hie bestat.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="69">
					<app>
						<lem wit="#Transcription">ez w&aelig;r min tot.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="70">
					<app>
						<lem wit="#Transcription">liez er sich geharre</lem>
