		</front>
		<body>
			<head>
				<title>
					<app>
						<lem wit="#Transcription">R 27 (Ein ander)</lem>
						<rdg wit="#Translation"><!--TITLE'S TRANSLATION--></rdg>
					</app>
				</title>
			</head>
			<lg type="stanza" n="1">
				<l n="1">
					<app>
						<lem wit="#Transcription">Chint, bereittet iuch der sliten u daz eis.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="2">
					<app>
						<lem wit="#Transcription">ia ist der leid winder chalt,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="3">
					<app>
						<lem wit="#Transcription">der hat uns der wnnechlichen blumen vil benomen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="4">
					<app>
						<lem wit="#Transcription">manger grunen linden stet ir tolden gris,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="5">
					<app>
						<lem wit="#Transcription">unbesungen ist der walt.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="6">
					<app>
						<lem wit="#Transcription">daz ist allez von des riffen ungenaden chomen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="7">
					<app>
						<lem wit="#Transcription">mugt ir schowen wie er hat die haid erzogen?</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="8">
					<app>
						<lem wit="#Transcription">diu ist von sinen schulden val.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="9">
					<app>
						<lem wit="#Transcription">dar zu sint die nahtigal</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="10">
					<app>
						<lem wit="#Transcription">alle ir wech gevlogen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="2">
				<l n="11">
					<app>
						<lem wit="#Transcription">Wol bedorft ich miner wisen vriunde rat</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="12">
					<app>
						<lem wit="#Transcription">umb ein dinch, als ich iu sag,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="13">
					<app>
						<lem wit="#Transcription">wa diu chint, daz si ri&esup;ten, ir vreuden solten phlegen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="14">
					<app>
						<lem wit="#Transcription">Megenwart ein wit stuben hat.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="15">
					<app>
						<lem wit="#Transcription">ob ez iu allen wol behag,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="16">
					<app>
						<lem wit="#Transcription">dar sil wir den gufenancz des veiertages legen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="17">
					<app>
						<lem wit="#Transcription">ez ist siner tohter wille, chom wir dar.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="18">
					<app>
						<lem wit="#Transcription">ir sulcz alle an ander sagen,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="19">
					<app>
						<lem wit="#Transcription">einen tanz alumb den chragen,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="20">
					<app>
						<lem wit="#Transcription">den bruvet Engelmar.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="3">
				<l n="21">
					<app>
						<lem wit="#Transcription">Der nah Chunegunde ge, des wert enein:</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="22">
					<app>
						<lem wit="#Transcription">der was ie nah tanze we.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="23">
					<app>
						<lem wit="#Transcription">ez wirt uns verwizzen, ist daz man ir niht enseit.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="24">
					<app>
						<lem wit="#Transcription">Geisel, ginch nah Ieuten hin und sag in zwein,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="25">
					<app>
						<lem wit="#Transcription">sprich daz &aeligcap;lle mit in ge.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="26">
					<app>
						<lem wit="#Transcription">ez ist zwischen mir und in ein starchiu sicherheit.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="27">
					<app>
						<lem wit="#Transcription">chint, vergiz durch niemen H&aelig;dewigen da,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="28">
					<app>
						<lem wit="#Transcription">bit si balde mit in gan.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="29">
					<app>
						<lem wit="#Transcription">einen sit si schulen lan,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="30">
					<app>
						<lem wit="#Transcription">daz binden uf die bra.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="4a">
				<l n="31">
					<app>
						<lem wit="#Transcription">Ich rat allen guten wiben uber al,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="32">
					<app>
						<lem wit="#Transcription">die der mazze wellent sin,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="33">
					<app>
						<lem wit="#Transcription">daz si hohgemu&esup;ten mannen holdez herze tragen,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="34">
					<app>
						<lem wit="#Transcription">ruch ez vorn hoher, hinden hin ze tal,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="35">
					<app>
						<lem wit="#Transcription">deche baz daz n&aelig;chelin.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="36">
					<app>
						<lem wit="#Transcription">warzu&esup; sol ein tehtir an ein collir umbe den chragen?</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="37">
					<app>
						<lem wit="#Transcription">diu wip sint sicher umbe daz haubet her gewesen,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="38">
					<app>
						<lem wit="#Transcription">so daz in niemen brach.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="39">
					<app>
						<lem wit="#Transcription">swaz in anderswa geschach,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="40">
					<app>
						<lem wit="#Transcription">des sint' ouch genesen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="4b">
				<l n="41">
					<app>
						<lem wit="#Transcription">Got gebeit den iungen wiben uber al,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="42">
					<app>
						<lem wit="#Transcription">die der mazze wellent sin,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="43">
					<app>
						<lem wit="#Transcription">daz si hohgemuten mannen holdez hercze tragen,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="44">
					<app>
						<lem wit="#Transcription">hoher vorn un hinden hin ze tal,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="45">
					<app>
						<lem wit="#Transcription">dechen baz daz n&aelig;chelin,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="46">
					<app>
						<lem wit="#Transcription">wazu sol ein tehtir an ein kollir umb den chragen?</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="47">
					<app>
						<lem wit="#Transcription">wip sint sicher umb daz houb her gewesen,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="48">
					<app>
						<lem wit="#Transcription">daz et in daz niemen brach.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="49">
					<app>
						<lem wit="#Transcription">swaz in anderswo geschach,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="50">
					<app>
						<lem wit="#Transcription">des sint si genesen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="5">
				<l n="51">
					<app>
						<lem wit="#Transcription">Eppe der zuht Geppen Gumpen ab der hant,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="52">
					<app>
						<lem wit="#Transcription">des half im sin drischelstap.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="53">
					<app>
						<lem wit="#Transcription">doch geschied ez mit der reutel meister Adelper.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="54">
					<app>
						<lem wit="#Transcription">daz was allez umb ein ay, daz Rupreht vant</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="55">
					<app>
						<lem wit="#Transcription">- ja w&aelig;n imz der tyevel gap -,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="56">
					<app>
						<lem wit="#Transcription">da mit drot er im ze werfen allez iene her.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="57">
					<app>
						<lem wit="#Transcription">einer der was beidiu zornich unde chal,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="58">
					<app>
						<lem wit="#Transcription">ubelichen sprach er "tracz".</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="59">
					<app>
						<lem wit="#Transcription">Rupreht warf im an den glacz,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="60">
					<app>
						<lem wit="#Transcription">daz ez ran hin ze tal.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="6">
				<l n="61">
					<app>
						<lem wit="#Transcription">Hie enphor do stunt so schone mir min har,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="62">
					<app>
						<lem wit="#Transcription">umb und umbe gie der span.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="63">
					<app>
						<lem wit="#Transcription">des vergaz ich sit man mich ein hous besorgen hiez,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="64">
					<app>
						<lem wit="#Transcription">salcz un chorn muz ich choufen durch daz iar.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="65">
					<app>
						<lem wit="#Transcription">we, waz het ich im getan,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="66">
					<app>
						<lem wit="#Transcription">der mich tumben ie von erst in disen chumber stiez?</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="67">
					<app>
						<lem wit="#Transcription">mine schulde waren chleine wider in.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="68">
					<app>
						<lem wit="#Transcription">mine vluech sint niht ze smal,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="69">
					<app>
						<lem wit="#Transcription">swanne ich da ze Riwental</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="70">
					<app>
						<lem wit="#Transcription">unberaten bin.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" m="7">
				<l n="71">
					<app>
						<lem wit="#Transcription">Frideli&esup;p bei Go&esup;telinde wolde gan,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="72">
					<app>
						<lem wit="#Transcription">des gedaht her Engelmar.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="73">
					<app>
						<lem wit="#Transcription">wil iuch niht verdriezzen, ich sag iu daz ende gar.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="74">
					<app>
						<lem wit="#Transcription">Eberhart der mayer must ez understan,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="75">
					<app>
						<lem wit="#Transcription">der wart zu der su&esup;ne braht.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="76">
					<app>
						<lem wit="#Transcription">anders w&aelig;r ir beider hende an ander in daz har.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="77">
					<app>
						<lem wit="#Transcription">zwein vil &oelig;den ganzen gent si vil gelich</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="78">
					<app>
						<lem wit="#Transcription">gein einander al den tach.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="79">
					<app>
						<lem wit="#Transcription">der des vorsingens phlach,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="80">
					<app>
						<lem wit="#Transcription">daz was Friderich.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
