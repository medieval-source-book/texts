	<text>
		<front>
			<head>
				<title>
					<app>
						<lem wit="#Transcription">Ein ander wis</lem>
						<rdg wit="#Translation"><!--TITLE'S TRANSLATION--></rdg>
					</app>
				</title>
			</head>
		</front>
		<body>
			<lg type="stanza" n="1">
				<l n="">
					<app>
						<lem wit="#Transcription">"Owe mir dirre not",</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">sprach ein wip, "der sumer wil zergan.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">des gewinne ich liht noch ein grabez har.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">ich sieh die blumen rot</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">vor dem walde trourichlichen stan.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">die heten also liehten schin, nu vælbent aber gar.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">nu mohten uns die blumen also schone sin bliben,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">seht, der wurde mir vil leiht ein chranz,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">wand ir glancz</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">hat mir miner swære vil vertriben."</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="2">
				<l n="">
					<app>
						<lem wit="#Transcription">Diu haid ist gar verblu&esup;t.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">di roten tolden risent valbe nider.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">daz machent in die sorgen, di si zu dem riffe hant.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">owe, wie si der mu&esup;t,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">er aucholf! chumt der sumer immer wider,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">der machet si so wol gevar, daz' aber schon stant.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">muscha mirz, wi ez Gisel da mit tancz tischen sol!</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">seht, des hilfet Ieutel, Perhtel, Ierengart.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">Eberhart</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">der gat an ir hant: seht, so ist im wol.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="3">
				<l n="">
					<app>
						<lem wit="#Transcription">Ich chom an ein stat,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">triwem, do was hobscher chinde vil.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">si heten einen tanz, der was dem vlecze gar ze wit.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">zu&esup; ainer ich getrat.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">ir muter sprach: "waz ob ich des niht wil,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">daz ir mit iht raunet? woy, daz ir verwazzen sit!</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">lat si mit genaden, zechet anderthalben hin!</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">ir horet wol, daz si mit iu niht rounen chan.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">aller man</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">lat si vri, die weil ich lebendich bin."</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="4">
				<l n="">
					<app>
						<lem wit="#Transcription">"Muter, zurnet niht,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">machet mir daz beiten niht ze lanch.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">beitet unz morgen, seht, so muz ich im veragen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">als in min oug ansiht,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">von im so treit mich aller min gedanch.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">des gat mir not, ia ward ich vert vil wol durch in zerslagen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">we, wie ez mir versmahet, daz er mir durch in geschah.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">vrowe, nu&esup; wis im durch minen willen gram.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">[..............]</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">ia ist erz, den ie min lip versprach."</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="5">
				<l n="">
					<app>
						<lem wit="#Transcription">Si hat sich min erwert.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">wie rehte chaum si daz hat getan!</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">si zeiget mir den wolves zant da si vil eben saz.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">ob si nu wol gevert,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">so muzz ouch mir min dinch nah heil ergan,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">zwinss ich huit, ia geseih ich leihte morgen baz.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">argiu wip gelonent sælten guten mannen wol.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">swer diu triwe su&esup;chet da ir lutzel ist,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">daz ist ein list,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">der si doch vil chleine helfen sol.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="6">
				<l n="">
					<app>
						<lem wit="#Transcription">Nu sorge ich hinger mich,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">wi sih min vrowe immer an mir bewar.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">ich weiz wol, si denchet min, in swelher mazze ez si.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">si sundet anders sich,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">wan ich mit grozzen triwen von ir var.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">getorst ich, ia wær ich ir zallen ziten gerne bei.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">ich widersitz en salman, in des handen si da stat,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">er sel si anderthalben hin danne ich si bit.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">solhen sit</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">man da heim in miner pharre hat.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="7">
				<l n="">
					<app>
						<lem wit="#Transcription">Min schimphen half an ir.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">do si des zornes mu&esup;tes widerwant,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">si braht mich des inne, daz ir zurnen was ein troum.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">vriunde wurde wir.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">si gie des tages vil gar an miner hant,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">di weil ich bei dem tanze was, des nam ir macze goum.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">si sprach: "vrowe, tut sin niht, ir chomts in grozzen nit."</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">mit der rede chund si irz verbieten nie.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">an diu chnie</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="">
					<app>
						<lem wit="#Transcription">braht mir diu sælbe dieren sit.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
		</body>
	</text>
</TEI>