	<text>
		<front>
			<head>
				<title type="main">
					<app>
						<lem wit="#Transcription"><!--Title as should be displayed above text. Put nothing here if the title in the text matches the title used elsewhwere--></lem>
						<rdg wit="#Translation"><!--Title as should be displayed above text. Put nothing here if the title in the text matches the title used elsewhwere--></rdg>
					</app>
				</title>
			</head>
		</front>
		<body>
			<lg>
                <l n="1">
					<app>
						<lem wit="#Transcription">„Klimberče mladý, praviť krátce,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="2">
					<app>
						<lem wit="#Transcription">pomni, [žes]<note anchored="true" type="critical">žes] <hi rend="italic">doplněno Lupáčem </hi></note>měl dobrého otce,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="3">
					<app>
						<lem wit="#Transcription">jehož nikto zlým nevinil</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="4">
					<app>
						<lem wit="#Transcription">a mnoho dobrého činil.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="5">
					<app>
						<lem wit="#Transcription">A sáms také rytieř smělý,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="6">
					<app>
						<lem wit="#Transcription">mladý, dobrý druh dospělý,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="7">
					<app>
						<lem wit="#Transcription">vědě <note type="critical" anchored="true">vědě] Wiediem <hi rend="italic">tisk</hi></note>, že mne neodvedeš,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="8">
					<app>
						<lem wit="#Transcription">než úfámť, že mne dovedeš,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="9">
					<app>
						<lem wit="#Transcription">kdež pokuši svého meče.“</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="10">
					<app>
						<lem wit="#Transcription">K králi vece: „Nemiež péče.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="11">
					<app>
						<lem wit="#Transcription">Tvé slovo, jaks řekl, zuostane</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="12">
					<app>
						<lem wit="#Transcription">a se tak líbost tvá stane.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="13">
					<app>
						<lem wit="#Transcription">Popros Boha za svou duši,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="14">
					<app>
						<lem wit="#Transcription">boď oř, v ten čas sobě tuši</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="15">
					<app>
						<lem wit="#Transcription">tam, kdež se již, jako známy,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="16">
					<app>
						<lem wit="#Transcription">jeliž v soudný den shledámy.“</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="17">
					<app>
						<lem wit="#Transcription">Jakž to vece, tak se zdrže,<note type="critical" anchored="true">zdrže] zdrzyeze <hi rend="italic">tisk</hi></note></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="18">pan Henrich se po něm zdviže.</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="19">S ním šel 
                    <note n="4">
                        šel] ſe 
                        <hi rend="italic">tisk </hi></note>na harc podlé toho</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="20">ctné dobré družiny mnoho,</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="21">jeho hesla volajíce,</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="22">v srdci tuto mysl majíce,</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="23">žeť nelze podlé své hospody</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="24">dojíti smrtedlné škody.</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="25">Nepřítelé sobě stesku,</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="26">když Supovo křídle blesku,</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="27">pod němaž ten rytíř drahý</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="28">křičieše své heslo: „Z Prahy!“</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="29">I hna v nepřátelský hluk</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="30">bez rozpači jako kluk,</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="31">přeprudce rozmáchav ony</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="32">přežádúcie zlaté 
                    <note n="5">
                        zlaté] zlatto 
                        <hi rend="italic">tisk </hi></note>zvony,</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="33">jimaž oř upomínáše,</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="34">na němžto se vzpodjímáše,</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="35">dělaje rytířské skutky</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="36">svým nepřátelům pro smutky,</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="37">dávaje jim hrozné rány</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="38">skrze tvrdé, husté brány,</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="39">čině ořem i sám sebú,</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="40">nelituje rukú obú,</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="41">jimaž zabyle kvapieše,</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="42">což kde jima polapieše,</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="43">to všecko bylo zkaženo,</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="44">sečeno i rozraženo.</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="45">Jeho dobrodrustva zřiece,</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="46">snažiechu se tu vždy viece</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="47">ty všecky, ktož podlé něho</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="48">byli hlédajíce jeho,</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="49">srazichu se s nepřátely</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="50">svými rytieřskými diely,</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="51">obcházejíc se uctivě,</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="52">noži, meči pamětlivě</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="53">sekúc, bodúc ze vší síly,</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="54">z ořóv ostrohami žíly</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="55">derúce, bodúce na ně,</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="56">chtíc sobě dobyti pláně</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="57">a k lepši se bráti spíše.</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="58">Tu Červená ruože ktvieše,</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="59">majíc rubínovo stkvěnie.</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="60">Jádro její podlé chtění</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="61">z arabského zlatta stkvieše,</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="62">pod nížto rytíř se držieše,</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="63">husté cesty v nepřátelech čině</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="64">– toť náleží ctné hrdině –</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="65">i nevyhybujíc se nikomu,</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="66">vždy se bera k miestu tomu,</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="67">kde by ten skutek ukázal,</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="68">ač by o něm kdo otázal,</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="69">by to bylo u paměti</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="70">i to mohli pověděti</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="71">dobří lidé bez příroka</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="72">na každý čas, rok od roka,</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="73">až ta Róže tak osvětla,</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="74">jakž při Lvu vždy věrně květla.</l>
					<app>
						<lem wit="#Transcription"></lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="75">
					<app>
						<lem wit="#Transcription">Tu svietieše Zlaté <note type="critical" anchored="true">Zlaté] zlatte <hi rend="italic">tisk </hi></note>kolo,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="76">
					<app>
						<lem wit="#Transcription">pod nímž nebieše úpolo</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="77">
					<app>
						<lem wit="#Transcription">mladému dobrému druhu</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="78">
					<app>
						<lem wit="#Transcription">zpodjéti tak práci tuhú:</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="79">
					<app>
						<lem wit="#Transcription">Klimberkovi šlechetnému,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="80">
					<app>
						<lem wit="#Transcription">v svém životě udatnému,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="81">
					<app>
						<lem wit="#Transcription">jenž nedbaje nic o škodě,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="82">
					<app>
						<lem wit="#Transcription">věrně sloužil své hospodě,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="83">
					<app>
						<lem wit="#Transcription">pomně na otce, na děda,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="84">
					<app>
						<lem wit="#Transcription">nepřátelóm bieše běda;</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="85">
					<app>
						<lem wit="#Transcription">každýho, kam se obrátil,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="86">
					<app>
						<lem wit="#Transcription">bil, sekl, buodl, v hlavy mlátil.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="87">
					<app>
						<lem wit="#Transcription">A pan Valkún z Bořešína</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="88">
					<app>
						<lem wit="#Transcription">věda, že boj ctná novina,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="89">
					<app>
						<lem wit="#Transcription">dá nejednu prudkú ránu,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="90">
					<app>
						<lem wit="#Transcription">vždy se boda po svém pánu.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="91">
					<app>
						<lem wit="#Transcription">Z Rožďalovic Ješek mladý</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
                <l n="92">
					<app>
						<lem wit="#Transcription">podlé své rytířské vnady</lem>
						<rdg wit="#Translation"></rdg>
					</app>
