		<body>
			<head>
				<title>
					<app>
						<lem wit="#Transcription">XI. TUNDALUS.</lem>
						<rdg wit="#Translation"><!--TITLE'S TRANSLATION--></rdg>
					</app>
				</title>
			</head>
			<lg type=section n="1">
				<head n="1">
					<app>
						<lem wit="#Transcription">Vaz tundalu&slong; hat ge&sling;in.</lem>
						<rdg wit="#Translation"><!--STANZA TITLE TRANSLATION--></rdg>
					</app>
				</head>
				<lg type=subsection n="1">
					<l n="1">
						<app>
							<lem wit="#Transcription">Gode&slong; wnder &slong;int manicfalt</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="2">
						<app>
							<lem wit="#Transcription">Di er uvidene hat ge&slong;talt</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="3">
						<app>
							<lem wit="#Transcription">Bit &slong;iner grozer crefte.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="4">
						<app>
							<lem wit="#Transcription">Wolden wir merken rechte</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="5">
						<app>
							<lem wit="#Transcription">Vnde uerneimen der heiligen &slong;rifte wort,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="6">
						<app>
							<lem wit="#Transcription">Wir ne &slong;prechin minner vbel wort.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="7">
						<app>
							<lem wit="#Transcription">Nu i&slong;t di arme men&slong;heit</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="8">
						<app>
							<lem wit="#Transcription">Al &slong;o cranc vn&bar; di brodekeit,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="9">
						<app>
							<lem wit="#Transcription">Daz &slong;i &slong;ich umbewollen</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="10">
						<app>
							<lem wit="#Transcription">Inkan behude vollen,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="11">
						<app>
							<lem wit="#Transcription">Got in du iz bit &slong;in&er; craft.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="12">
						<app>
							<lem wit="#Transcription">Di we&slong;&slong;agin hant un&slong; ge&slong;acht</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="13">
						<app>
							<lem wit="#Transcription">Vzer der gode&slong; lere</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="14">
						<app>
							<lem wit="#Transcription">Daz eim riche &slong;und&er;e</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="15">
						<app>
							<lem wit="#Transcription">Daz himelriche &slong;i al&slong;o unkunde</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="16">
						<app>
							<lem wit="#Transcription">Al&slong;e &slong;i eime olbendin</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="17">
						<app>
							<lem wit="#Transcription">Daz er &slong;ih konne gebovgen</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="18">
						<app>
							<lem wit="#Transcription">Durh d&er; nalden ovgen.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="19">
						<app>
							<lem wit="#Transcription">Daz i&slong;t enge&slong;tlih gnuch!</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="20">
						<app>
							<lem wit="#Transcription">Och &slong; kundent un&slong; di buch</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="21">
						<app>
							<lem wit="#Transcription">'Vix iu&slong;t&usmod; &slong;aluabit&ra;.'</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="22">
						<app>
							<lem wit="#Transcription">Daz virnemet alden vn&bar; iunc!</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="23">
						<app>
							<lem wit="#Transcription">Daz quid daz van manne noch von wibe</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="24">
						<app>
							<lem wit="#Transcription">Di gerehtin kume beliben.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="25">
						<app>
							<lem wit="#Transcription">Her wid&er; &slong;o i&slong;t un&slong; ge&slong;at</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="26">
						<app>
							<lem wit="#Transcription">Git tro&slong; an einer ander &slong;tat:</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="27">
						<app>
							<lem wit="#Transcription">'Nolo mort&emacr; pec&bar;c&bar;ori&slong;.'</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="28">
						<app>
							<lem wit="#Transcription">Got &slong;prichit, de&slong; &slong;underi&slong; dodi&slong;</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="29">
						<app>
							<lem wit="#Transcription">Inwolle er nit, wene daz er lebe</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="30">
						<app>
							<lem wit="#Transcription">Vn&bar; &slong;ich &slong;in&er; &slong;unden &slong;uldic gebe</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="31">
						<app>
							<lem wit="#Transcription">Vn&bar; &slong;ih betalle trabe kere.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="32">
						<app>
							<lem wit="#Transcription">Nu &slong;old ir virnemen mere,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="33">
						<app>
							<lem wit="#Transcription">War umbe ich der reiden begunde.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="34">
						<app>
							<lem wit="#Transcription">Ich han if gut urkunde</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="35">
						<app>
							<lem wit="#Transcription">Von gelerden vn&bar; och con leigin,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="36">
						<app>
							<lem wit="#Transcription">Daz ich ane &slong;meichin</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="37">
						<app>
							<lem wit="#Transcription">In duz&slong;en &slong;age di warheit</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="38">
						<app>
							<lem wit="#Transcription">Al&slong; iz in latinen ge&slong;riben &slong;teit</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="39">
						<app>
							<lem wit="#Transcription">Von eime manne wol bekant.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="40">
						<app>
							<lem wit="#Transcription">D&er; wa&slong; tundalu&slong; genant.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="41">
						<app>
							<lem wit="#Transcription">Der wa&slong; ein man vil mi&slong;&slong;etedic:</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="42">
						<app>
							<lem wit="#Transcription">Got wart ime &slong;int genedik.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="43">
						<app>
							<lem wit="#Transcription">Dri tage er in brodin lach:</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="44">
						<app>
							<lem wit="#Transcription">Sin gei&slong;t wr su d&er; hellen un&bar; &slong;ach</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="45">
						<app>
							<lem wit="#Transcription">Manege dink der er wart wi&slong;.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="46">
						<app>
							<lem wit="#Transcription">Och quam er in daz paradi&slong;</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="47">
						<app>
							<lem wit="#Transcription">Da er irkande godi&slong; dowgen</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="48">
						<app>
							<lem wit="#Transcription">Wole bit &slong;inen | owgen,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="49">
						<app>
							<lem wit="#Transcription">Di er &slong;int &slong;ageta offenbare.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="50">
						<app>
							<lem wit="#Transcription">Nu horiet in welcheme iare</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="51">
						<app>
							<lem wit="#Transcription">Ge&slong;cehe di&slong; mere.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="52">
						<app>
							<lem wit="#Transcription">Eilif hundirt vn&bar; nune un&bar; virzik iare,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="53">
						<app>
							<lem wit="#Transcription">Daz i&slong;t war, de&slong; do waren,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="54">
						<app>
							<lem wit="#Transcription">Daz geborin wart got un&slong;er herre.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="55">
						<app>
							<lem wit="#Transcription">Nu wil ich &slong;agen uon dem man</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="56">
						<app>
							<lem wit="#Transcription">Von deme ich d&er; reiden began.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="57">
						<app>
							<lem wit="#Transcription">Ybernen i&slong;t ein ein <hi rend="italic">ein</hi>lant,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="58">
						<app>
							<lem wit="#Transcription">Inwei&slong;ten uffe daz mere gewant.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="59">
						<app>
							<lem wit="#Transcription">Su&bar;den an orden dar iz &slong;teit.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="60">
						<app>
							<lem wit="#Transcription">Dar umbe geint wazz&er; vil breit,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="61">
						<app>
							<lem wit="#Transcription">Daz gebirge groz un&bar; daz geuilde.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="62">
						<app>
							<lem wit="#Transcription">Di lude &slong;int da harte milde,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="63">
						<app>
							<lem wit="#Transcription">Ir&slong; gemude&slong; &slong;int &slong;i vro&bar;</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="64">
						<app>
							<lem wit="#Transcription">Daz lant i&slong;t milche un&bar; honege&slong; vol</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="65">
						<app>
							<lem wit="#Transcription">Inde fruchte, *slong; man &slong;agit</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="66">
						<app>
							<lem wit="#Transcription">Beide vi&slong;&slong;e vn&bar; iaget.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="67">
						<app>
							<lem wit="#Transcription">Mer wine&slong; in konnen &slong;i nit gewinnen.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="68">
						<app>
							<lem wit="#Transcription">Da i&slong;t vile &slong;langen, credin, &slong;pinnen:</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="69">
						<app>
							<lem wit="#Transcription">Doch &slong;o hat ir holz di craft</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="70">
						<app>
							<lem wit="#Transcription">Daz iz alliz virgipni&slong;&slong;e uber winden mach:</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="71">
						<app>
							<lem wit="#Transcription">So iz wirt virtriben dan.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="72">
						<app>
							<lem wit="#Transcription">Da &slong;int gude wib vn&bar; man.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="73">
						<app>
							<lem wit="#Transcription">Si hant gude wapen un&bar; gewant</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="74">
						<app>
							<lem wit="#Transcription">In&bar; wonent vil na engelant,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="75">
						<app>
							<lem wit="#Transcription">Naher den &slong;otten</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="76">
						<app>
							<lem wit="#Transcription">Dan den britten,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="77">
						<app>
							<lem wit="#Transcription">Quo&slong; q&combsgvertl;da&bar; galen&slong;e&slong; uocant,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="78">
						<app>
							<lem wit="#Transcription"><hi rend="italic">Di galen&slong;e&slong; &slong;int genant.</hi></lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="79">
						<app>
							<lem wit="#Transcription">Der wec i&slong; dannen intlazen</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="80">
						<app>
							<lem wit="#Transcription">Zu wieden un&bar; zu &slong;trazen</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="81">
						<app>
							<lem wit="#Transcription">Vn&bar; ein deil in hi&slong;pangen gewant.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="82">
						<app>
							<lem wit="#Transcription">Ibernen daz &slong;elbe einlant</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="83">
						<app>
							<lem wit="#Transcription">Hat vir un&bar; drizcik howbet&slong;tede</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="84">
						<app>
							<lem wit="#Transcription">Di alle &slong;tent an zweir vreden.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="85">
						<app>
							<lem wit="#Transcription">Eine &slong;tat heizet archamacha,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="86">
						<app>
							<lem wit="#Transcription">Di &slong;tet yb&er;nen och wol na,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="87">
						<app>
							<lem wit="#Transcription">Di &slong;aget man daz &slong;i vil riche &slong;i.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="88">
						<app>
							<lem wit="#Transcription">Crocagen&slong;i&slong; &slong;tet och da bi.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="89">
						<app>
							<lem wit="#Transcription">Da inne &slong; wa&slong; ge&slong;ezzen</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="90">
						<app>
							<lem wit="#Transcription">Ein ridder wol virmezzin;</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="91">
						<app>
							<lem wit="#Transcription">Er wa&slong; edele un&bar; wole bekant:</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="92">
						<app>
							<lem wit="#Transcription"><hi rend="italic">D&er; wa&slong; tundalu&slong; genant.</hi></lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
				</lg>
				<lg type=subsection n="2">
					<l n="93">
						<app>
							<lem wit="#Transcription">. . . uan</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="94">
						<app>
							<lem wit="#Transcription">Bit d&er; ewiger qualen ungemach.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="95">
						<app>
							<lem wit="#Transcription">Zu deme engele daz &slong;i &slong;prah:</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="96">
						<app>
