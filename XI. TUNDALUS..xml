					</l>
					<l n="204">
						<app>
							<lem wit="#Transcription">Daz 8ich hi al&slong;o grozlich &slong;in,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="205">
						<app>
							<lem wit="#Transcription">Solin &slong; dich &slong;huen un&bar; vlin,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="206">
						<app>
							<lem wit="#Transcription">War umbe gei&slong;tu in &slong;o na?'</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="207">
						<app>
							<lem wit="#Transcription">Der engel antwrtte al da</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="208">
						<app>
							<lem wit="#Transcription">'Vn&slong;e vart &slong;al ande&er;e&slong; &slong;in getan,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="209">
						<app>
							<lem wit="#Transcription">Wir &slong;olin in noch narre &slong;tan.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="210">
						<app>
							<lem wit="#Transcription">Si muzzen uz irweilid we&slong;en</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="211">
						<app>
							<lem wit="#Transcription">Di hi uore &slong;olen gene&slong;en.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="212">
						<app>
							<lem wit="#Transcription">Dit dir i&slong;t acheron genant.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="213">
						<app>
							<lem wit="#Transcription">Ich &slong; dir &slong;agen al zu hant</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="214">
						<app>
							<lem wit="#Transcription">Vil rehte daz getude:</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="215">
						<app>
							<lem wit="#Transcription">Iz w&er;&slong;lindet alle gire lude.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="216">
						<app>
							<lem wit="#Transcription">Div &slong;rift un&slong; uon ime kund dut,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="217">
						<app>
							<lem wit="#Transcription">Iz &slong;ole uir&slong;linde groze vlut</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="218">
						<app>
							<lem wit="#Transcription">Vn&bar; in &slong;al iz ken wnd&er; han</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="219">
						<app>
							<lem wit="#Transcription">Ob in &slong;inen mund vlize d&er; iordan.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="220">
						<app>
							<lem wit="#Transcription">Noch wil ich dich machen wi&slong;.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="221">
						<app>
							<lem wit="#Transcription">Di&slong;e zwenen man di du hi &slong;i&slong;</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="222">
						<app>
							<lem wit="#Transcription">In &slong;inen munde v&er;keren &slong;ich,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="223">
						<app>
							<lem wit="#Transcription">Daz waren zwene ri&slong;en vre&slong;lich.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="224">
						<app>
							<lem wit="#Transcription">Di in hadden in irme liche</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="225">
						<app>
							<lem wit="#Transcription">Nit gelebet &slong;o eweliche</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="226">
						<app>
							<lem wit="#Transcription">Al&slong;e hadde bede di gine,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="227">
						<app>
							<lem wit="#Transcription">Di nu geuaren &slong;in hine,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="228">
						<app>
							<lem wit="#Transcription">Der namen du wola ha&slong;t bekant.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="229">
						<app>
							<lem wit="#Transcription">Der eine i&slong;t ferrugi&usmod; genant,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="230">
						<app>
							<lem wit="#Transcription">Der and&er; hezzit &slong;onali&usmod;.'</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="231">
						<app>
							<lem wit="#Transcription">Div &slong;ele antwirtte ime al&slong;u&slong;</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="232">
						<app>
							<lem wit="#Transcription">'Wi &slong;ere ich de&slong; bewegit bin</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="233">
						<app>
							<lem wit="#Transcription">Daz di&slong;e hadde &slong;o guden &slong;in,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="234">
						<app>
							<lem wit="#Transcription">Daz ruwen ire &slong;unden,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="235">
						<app>
							<lem wit="#Transcription">Vn&bar; du un de&slong; gi&slong;t urkunde,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="236">
						<app>
							<lem wit="#Transcription">Vn&bar; &slong;i dirren pinen &slong;int benomen! </lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="237">
						<app>
							<lem wit="#Transcription">Harre min, wi i&slong;t dit komen,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="238">
						<app>
							<lem wit="#Transcription">Daz di&slong;e and&er;e zwene gigande</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="239">
						<app>
							<lem wit="#Transcription">Stent in fi&slong;&slong;e&slong; dieri&slong; banden</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="240">
						<app>
							<lem wit="#Transcription">Vn&bar; v&er;kerit in &slong;ime munde?'</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="241">
						<app>
							<lem wit="#Transcription">Der engel antw&er;tten begunde</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="242">
						<app>
							<lem wit="#Transcription">'Di&slong;en oine un&bar; di&slong;e quale, &horbar;</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="243">
						<app>
							<lem wit="#Transcription">De&slong; inhat mich decheine hele, &horbar;</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="244">
						<app>
							<lem wit="#Transcription">Al ein dunket &slong;i dich groz hi &slong;hinen,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="245">
						<app>
							<lem wit="#Transcription">Du kume&slong; noch zu merren pinen</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="246">
						<app>
							<lem wit="#Transcription">Di du &slong;alt &slong;in un&bar; &slong;int bekant,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="247">
						<app>
							<lem wit="#Transcription">Er du wid&er; w&er;de&slong; gewant.'</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="248">
						<app>
							<lem wit="#Transcription">Er ne ge&slong;prach dit ni &slong;p &slong;ire</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="249">
						<app>
							<lem wit="#Transcription">SO ginc er nah&er; deme dire.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="250">
						<app>
							<lem wit="#Transcription">Di &slong;ele, al en dede &slong;i iz node da,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="251">
						<app>
							<lem wit="#Transcription">Si mu&slong;t ime uolgen na.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="252">
						<app>
							<lem wit="#Transcription">Ire wa&slong; vil &slong;ere leide.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="253">
						<app>
							<lem wit="#Transcription">Al&slong;e &slong;i da &slong;tunden beide</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="254">
						<app>
							<lem wit="#Transcription">Vor deme dire enge&slong;tlich,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="255">
						<app>
							<lem wit="#Transcription">Der engel intlichede &slong;ich</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="256">
						<app>
							<lem wit="#Transcription">Et&slong;ola reman&slong;it mi&dslong;era,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="257">
						<app>
							<lem wit="#Transcription">Inde &slong;i arme beleb al eine da.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="258">
						<app>
							<lem wit="#Transcription">Di tuuele di d&er; wizen plagen</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="259">
						<app>
							<lem wit="#Transcription">Doo &slong;i di &slong;ele alene &slong;agen,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="260">
						<app>
							<lem wit="#Transcription">Si uuren umbe &slong;i,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="261">
						<app>
							<lem wit="#Transcription">Ut cane&slong; rabidi,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="262">
						<app>
							<lem wit="#Transcription">Al&slong;e di dobende hunde</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="263">
						<app>
							<lem wit="#Transcription">Zugen &slong;i &slong;i zu de&slong; dire&slong; munde</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="264">
						<app>
							<lem wit="#Transcription">In den buch bit grozen &slong;legen</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="265">
						<app>
							<lem wit="#Transcription">So &slong;i mei&slong;t | mohten w&er;wegen.</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="266">
						<app>
							<lem wit="#Transcription">Wilche pine, wi grozen &slong;trit</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="267">
						<app>
							<lem wit="#Transcription">Si da lide in corz&er; cit,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="268">
						<app>
							<lem wit="#Transcription">Al einen &slong;i iz wole u&er;&slong;uigen mohte,</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="269">
						<app>
							<lem wit="#Transcription">Ein wi&slong; man d&er; id dohte</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
					<l n="270">
						<app>
							<lem wit="#Transcription">Ez mohte wole bit &slong;inen wizzen</lem>
							<rdg wit="#Translation"></rdg>
						</app>
					</l>
				</lg>
			</lg>
		</body>
