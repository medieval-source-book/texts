	<text>
		<front>
			<div>
				<listWit>
					<witness xml:id="Transcription">Ein ander</witness>
					<witness xml:id="Translation"><!--TITLE'S TRANSLATION--></witness>
				</listWit>
			</div>
		</front>
		<body>
			<head>
				<title>
					<app>
						<lem wit="#Transcription">Ein ander</lem>
						<rdg wit="#Translation"><!--TITLE'S TRANSLATION--></rdg>
					</app>
				</title>
			</head>
			<lg type="stanza" n="1">
				<l n="1">
					<app>
						<lem wit="#Transcription">Owe liebiu sumerzit,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="2">
					<app>
						<lem wit="#Transcription">daz er si verwazzen,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="3">
					<app>
						<lem wit="#Transcription">der mir din ze heil un ouch ze vrouden nine gunne!</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="4">
					<app>
						<lem wit="#Transcription">des ist mange herze gram,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="5">
					<app>
						<lem wit="#Transcription">daz nu trouren muz.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="6">
					<app>
						<lem wit="#Transcription">nu schowet, wie diu heide lit:</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="7">
					<app>
						<lem wit="#Transcription">der ist niht verlazzen</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="8">
					<app>
						<lem wit="#Transcription">aller blumen, da si mit ir scham verdechen chunne.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="9">
					<app>
						<lem wit="#Transcription">owe dem winder, der si ir benam!</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="10">
					<app>
						<lem wit="#Transcription">schir wirt uns bu&esup;z</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="11">
					<app>
						<lem wit="#Transcription">sin un aller der, di mir da die guten vromde machen,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="12">
					<app>
						<lem wit="#Transcription">den enwird ich nimmer inerchlichen holt.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="13">
					<app>
						<lem wit="#Transcription">Willechint und Amlolt</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="14">
					<app>
						<lem wit="#Transcription">habent mih beworren da mit lugelichen sachen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="2">
				<l n="15">
					<app>
						<lem wit="#Transcription">We warumbe tunt si daz?</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="16">
					<app>
						<lem wit="#Transcription">mohte sis verdriezzen!</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="17">
					<app>
						<lem wit="#Transcription">ia get ir min singen leider niht ze nahen</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="18">
					<app>
						<lem wit="#Transcription">als ez doch solde gan.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="19">
					<app>
						<lem wit="#Transcription">we ich selich man,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="20">
					<app>
						<lem wit="#Transcription">selten ich ir ie vergaz:</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="21">
					<app>
						<lem wit="#Transcription">moht ich sin geniezzen!</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="22">
					<app>
						<lem wit="#Transcription">iane chan mich doch min dienst niht vervahen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="23">
					<app>
						<lem wit="#Transcription">sine wil des niht verstan,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="24">
					<app>
						<lem wit="#Transcription">daz min lieber wan</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="25">
					<app>
						<lem wit="#Transcription">lit an ander nieman niwan an ir einer leibe</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="26">
					<app>
						<lem wit="#Transcription">ine gesten ir miner triwen nimmer ab.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="27">
					<app>
						<lem wit="#Transcription">nu seht, ob siz vur dienst hab.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="28">
					<app>
						<lem wit="#Transcription">si ist in minem herzen immer liebist aller wibe.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="3">
				<l n="29">
					<app>
						<lem wit="#Transcription">Disiu rede lige also,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="30">
					<app>
						<lem wit="#Transcription">lazzen wirs beliben.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="31">
					<app>
						<lem wit="#Transcription">sprechen, we, waz tun ab ich den tumben getelingen?</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="32">
					<app>
						<lem wit="#Transcription">von den han ich bosen wer,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="33">
					<app>
						<lem wit="#Transcription">swelhes endes ich var.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="34">
					<app>
						<lem wit="#Transcription">alle wæren si sin vro,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="35">
					<app>
						<lem wit="#Transcription">mohten si mich vertriben,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="36">
					<app>
						<lem wit="#Transcription">mich von minen vrouden un von lieber stat verdringen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="37">
					<app>
						<lem wit="#Transcription">wol ir, der min herze gert</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="38">
					<app>
						<lem wit="#Transcription">stille und offenbar!</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="39">
					<app>
						<lem wit="#Transcription">ine geloubt niht, daz si gehengen an ir vinden.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="39">
					<app>
						<lem wit="#Transcription">bezzer wær i, liezzen si mich ane not.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="41">
					<app>
						<lem wit="#Transcription">ich gemaches alle rot,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="42">
					<app>
						<lem wit="#Transcription">di da mit ir rounent, wellent si sein niht erwinden.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="4">
				<l n="43">
					<app>
						<lem wit="#Transcription">nt immer an dem veiertage</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="44">
					<app>
						<lem wit="#Transcription">so ist ir samenunge.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="45">
					<app>
						<lem wit="#Transcription">swaz der dorper ist in einem weiten umbesweife</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="45">
					<app>
						<lem wit="#Transcription">choment mit einander dar</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="46">
					<app>
						<lem wit="#Transcription">alle &uring;f minen tracz.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="47">
					<app>
						<lem wit="#Transcription">owe mir sines vil irren chragen!</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="48">
					<app>
						<lem wit="#Transcription">Fridepreht der iunge,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="49">
					<app>
						<lem wit="#Transcription">er greif ir an daz chinne: ine weiz nah weu der tore greif.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="50">
					<app>
						<lem wit="#Transcription">si vurhtent umb ein har</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="51">
					<app>
						<lem wit="#Transcription">niemens widersacz.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="52">
					<app>
						<lem wit="#Transcription">doh mach er und ettelicher da vil wol bestrouchen,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="53">
					<app>
						<lem wit="#Transcription">daz im bezzer wær, und het er ez verborn.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="54">
					<app>
						<lem wit="#Transcription">sines swertes helze vorn</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="55">
					<app>
						<lem wit="#Transcription">zart ir bei dem chrumben raien einen chleinen stouchen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="5">
				<l n="56">
					<app>
						<lem wit="#Transcription">Sa er an sinem geile spranch</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="57">
					<app>
						<lem wit="#Transcription">an dem umbeswanche,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="58">
					<app>
						<lem wit="#Transcription">do begreif ez in und zart in auz unz an daz ende.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="59">
					<app>
						<lem wit="#Transcription">owe daz man imz vertreit,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="60">
					<app>
						<lem wit="#Transcription">einem torchen chragen!</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="61">
					<app>
						<lem wit="#Transcription">ez ergie mit danche.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="62">
					<app>
						<lem wit="#Transcription">daz verwizzen im genuge ze einer missewende.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="63">
					<app>
						<lem wit="#Transcription">waz im leute widerseit!</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="64">
					<app>
						<lem wit="#Transcription">wil si sich bechlagen,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="65">
					<app>
						<lem wit="#Transcription">si gemachet im sin umbesæzzen vil ungenæme.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="66">
					<app>
						<lem wit="#Transcription">moht ein tore sin helze sleifen zu&eup;,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="67">
					<app>
						<lem wit="#Transcription">e daz ez solhen (...) schaden tu&esup;!</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="68">
					<app>
						<lem wit="#Transcription">wir vertrugen, daz sin swert in einem chorn læge.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="6">
				<l n="69">
					<app>
						<lem wit="#Transcription">Da ir bei einander sit,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="70">
					<app>
						<lem wit="#Transcription">tumbe getelinge,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="71">
					<app>
						<lem wit="#Transcription">da sult ir iuch huten wol vor einem torchen chnehte.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="72">
					<app>
						<lem wit="#Transcription">der get alrest hiwer hervur</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="73">
					<app>
						<lem wit="#Transcription">und ist geheizzen Per.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="74">
					<app>
						<lem wit="#Transcription">der ist lanch und ahselwit,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="75">
					<app>
						<lem wit="#Transcription">geitich ubler dinge,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="76">
					<app>
						<lem wit="#Transcription">und wnschet, daz er chom, da er im genuch gevehte.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="77">
					<app>
						<lem wit="#Transcription">er chumt choum her in ze der tuer.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="78">
					<app>
						<lem wit="#Transcription">phy, wer braht in her?</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="79">
					<app>
						<lem wit="#Transcription">der ist Hildeboldes swester sun von Pernreute.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="80">
					<app>
						<lem wit="#Transcription">der da wart erslagen umb ein ingewer.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="81">
					<app>
						<lem wit="#Transcription">daz tet Willeher.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="82">
					<app>
						<lem wit="#Transcription">er tu&esup;t mort, chumt er, da sih gesammet tumbe leute.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="7">
				<l n="83">
					<app>
						<lem wit="#Transcription">Allez Tulnære velt,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="84">
					<app>
						<lem wit="#Transcription">daz hat niht so tumbes</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="85">
					<app>
						<lem wit="#Transcription">so von der Treisem hin ze tal hin gein Zeizzenmo&usup;wer,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="86">
					<app>
						<lem wit="#Transcription">swa si sint die selben dri</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="87">
					<app>
						<lem wit="#Transcription">noch ist einer da,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="88">
					<app>
						<lem wit="#Transcription">dem get wol sein scheib enzælt</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="89">
					<app>
						<lem wit="#Transcription">slehtes unde chrumbes,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="90">
					<app>
						<lem wit="#Transcription">und ust doch von allen vier enden ein gebo&usup;wer.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="91">
					<app>
						<lem wit="#Transcription">er ist ir diche nahen bei,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="92">
					<app>
						<lem wit="#Transcription">do von so wierd ich gra.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="93">
					<app>
						<lem wit="#Transcription">er ist noh tumber danne die uns in den anger sprungen</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="94">
					<app>
						<lem wit="#Transcription">saht ir den, der Vrideroun ir spigel nam?</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="95">
					<app>
						<lem wit="#Transcription">iener der gebart alsam.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="96">
					<app>
						<lem wit="#Transcription">er ist ir einer, der mich hat von lieber stat verdrungen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="8">
				<l n="97">
					<app>
						<lem wit="#Transcription">Er treit eine busemsn&uring;r</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="98">
					<app>
						<lem wit="#Transcription">von alroten siden,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="99">
					<app>
						<lem wit="#Transcription">Fridepreht der iunge, und ein misencorden lange.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="100">
					<app>
						<lem wit="#Transcription">daz get hinden verre hin dan</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="101">
					<app>
						<lem wit="#Transcription">und ist chopherrot.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="102">
					<app>
						<lem wit="#Transcription">ich sag iu, wie er vert gevu&esup;r:</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="103">
					<app>
						<lem wit="#Transcription">das muz ich in niden.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="104">
					<app>
						<lem wit="#Transcription">do sich der tanz zelie und daz ez was in dem gedrange,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="105">
					<app>
						<lem wit="#Transcription">daz man siczen solde gan,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="106">
					<app>
						<lem wit="#Transcription">da er hin gebot,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="107">
					<app>
						<lem wit="#Transcription">ine gesah mir nie bei meinen iaren also leide</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="108">
					<app>
						<lem wit="#Transcription">als ich mir zewar an der vil guten sah,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="109">
					<app>
						<lem wit="#Transcription">di er uf ein rippe stach</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="110">
					<app>
						<lem wit="#Transcription">mit dem sælben mezzer, daz gie hinden u&osup;z der scheide.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
			<lg type="stanza" n="9">
				<l n="111">
					<app>
						<lem wit="#Transcription">Er treit einen mæchenich,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="112">
					<app>
						<lem wit="#Transcription">der snidet als ein schære,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="113">
					<app>
						<lem wit="#Transcription">und einen guten fridehu&esup;t von haselinen zeinen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="114">
					<app>
						<lem wit="#Transcription">einen vilz hat er dar</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="115">
					<app>
						<lem wit="#Transcription">uf also schon gezogen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="116">
					<app>
						<lem wit="#Transcription">ez schrotet mangen eisenrinch</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="117">
					<app>
						<lem wit="#Transcription">und machet wambeis lære</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="118">
					<app>
						<lem wit="#Transcription">swa ir sit, ir muget iuch wol mit eren ab im leinen.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="119">
					<app>
						<lem wit="#Transcription">dorper, nemt des sælben war:</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="120">
					<app>
						<lem wit="#Transcription">er heizzet Ilsunch.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="121">
					<app>
						<lem wit="#Transcription">sin swert, daz ist geluppet. er ist mort, den ez erreichet,</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="122">
					<app>
						<lem wit="#Transcription">der muz an der stat geligen tot.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="123">
					<app>
						<lem wit="#Transcription">ist daz niht ein grozziu not?</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
				<l n="124">
					<app>
						<lem wit="#Transcription">ez ist ein Wæildhovære, wol gehertet und geweichet.</lem>
						<rdg wit="#Translation"></rdg>
					</app>
				</l>
			</lg>
		</body>
